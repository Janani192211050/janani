str=input("enter the string:")
str1=str.lower()
a,s,spl=0,0,0
alpha=[]
special=[]
for i in range(len(str1)):
    if str1[i].isalpha():
        a=a+1
        alpha.append(str1[i])
    elif str1[i].isspace():
        s=s+1
    else:
        spl=spl+1
        special.append(str1[i])
x=alpha
y=x[::-1]
if (x==y):
    print("true")
else:
    print("false")
a=input("enter the string:")
b=a.split()
